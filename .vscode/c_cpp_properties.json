{
	"env" : {
		"myZephyrSourcePath": "~/zephyrproject/zephyr",
		"myZephyrSDKPath": "~/zephyr-sdk-0.16.8/"
	},
	"configurations": [
		{
			"name": "Linux",
			"includePath": [
				"${myZephyrSourcePath}/include/",
				"${myZephyrSourcePath}/lib/libc/minimal/include/",
				"${myZephyrSourcePath}/arch/arm/include/",
				"${workspaceFolder}/../Weather-Station/**"
			],
			"defines": [],
			"compilerPath": "${myZephyrSDKPath}/arm-zephyr-eabi/bin/arm-zephyr-eabi-gcc",
			"cStandard": "c17",
			"cppStandard": "gnu++17",
			"intelliSenseMode": "gcc-x64"
		}
	],
	"version": 4
}